alias xc=     'xclip -sel c'
alias apt=    'sudo apt'
alias reboot= 'sudo reboot'	
alias update= 'sudo apt update & apt upgrade'
alias ..='cd ..'
alias ...='cd ../../../'
alias ....='cd ../../../../'
alias .....='cd ../../../../'
alias .4='cd ../../../../'
alias .5='cd ../../../../..'
alias path='echo -e ${PATH//:/\\n}'
